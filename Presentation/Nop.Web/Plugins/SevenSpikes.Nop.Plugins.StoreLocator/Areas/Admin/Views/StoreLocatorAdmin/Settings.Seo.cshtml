@model StoreLocatorSettingsModel

<div class="panel-group">
    <div class="panel panel-default  advanced-setting">
        <div class="panel-body">
            @(Html.LocalizedEditor<StoreLocatorSettingsModel, StoreLocatorLocalizedModel>("store-locator-seo-localized",
                        @<div>
                            <div>
                                <input type="hidden" asp-for="@Model.Locales[item].LanguageId" />
                            </div>
                            <div class="form-group">
                                <div class="col-md-3">
                                    <nop-label asp-for="@Model.Locales[item].SeName" />
                                </div>
                                <div class="col-md-9">
                                    <nop-editor asp-for="@Model.Locales[item].SeName" />
                                    @Html.ValidationMessageFor(model => Model.Locales[item].SeName)
                                </div>
                            </div>
                        </div>
                        ,
                        @<div>
                            <div class="form-group">
                                <div class="col-md-3">
                                    <nop-label asp-for="SeName" />
                                </div>
                                <div class="col-md-9">
                                    <nop-editor asp-for="SeName" />
                                    <span asp-validation-for="SeName"></span>
                                </div>
                            </div>
                        </div>
                        ))
        </div>
    </div>
</div>